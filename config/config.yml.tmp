title: "My Blog"
dir: "./md"
port: 5006
env: dev
cache: 3

gitalk:
  client-id: "Your github oauth app client-id, required fields. eg: ad549a9d085d7f5736d3"
  client-secret: "Your github oauth app client-secret, required fields. eg: 510d1a6bb875fd5031f0d613cd606b1d"
  repo: "Github repo for gitalk, required fields. eg: blog-issue"
  owner: "Your github account, required fields."
  admin:
    - "Your github account"
  labels:
    - "Custom issue labels, eg: gitalk"

proxy:
  github-api: "Reverse proxy of api.github.com, optional."
  github-cors: "Replace gitalk's corsproxy, optional."
  google-ad: "Reverse proxy of pagead2.googlesyndication.com, optional."
  google-ay: "Reverse proxy of www.googletagmanager.com, optional."

sitemap:
  domain: "Youer website domain, eg: www.google.com"
  tls: "Whether to use TLS, default is true"
  cron: "Set a cron to update sitemap file, eg(每天早上8点): 0 0 8 * * *"
  path: "Custom sitemap router, default is /sitemap.xml"
  exist-file: "Set up Sitemap filepath, use this setting means mapping a static file and you cannot to update a sitemap with cron."

analyzer-baidu: "Your Baidu analyzer code"
analyzer-google: "Your Google analyzer code"
analyzer-google-ad: "Your Google AdSense code"

ignore-file:
  - "demo.md"
ignore-path:
  - "demo"